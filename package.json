{
  "scripts": {
    "build": "rollup -c",
    "dev": "npm-run-all plugin:unlink plugin:link -p plugin:watch plugin:debug",
    "plugin:watch": "rollup -c -w --watch.onEnd=\"npm run plugin:restart\"",
    "plugin:unlink": "flexcli plugin unlink --uuid com.energy.spotify_integration --silent",
    "plugin:link": "flexcli plugin link --path com.energy.spotify_integration.plugin --uuid com.energy.spotify_integration --start false",
    "plugin:restart": "flexcli plugin restart --uuid=com.energy.spotify_integration",
    "plugin:debug": "flexcli plugin debug --uuid=com.energy.spotify_integration",
    "plugin:validate": "flexcli plugin validate --path com.energy.spotify_integration.plugin",
    "plugin:pack": "flexcli plugin pack --path com.energy.spotify_integration.plugin",
    "plugin:install": "flexcli plugin install --path ./com.energy.spotify_integration.flexplugin --force"
  },
  "type": "commonjs",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.0",
    "glob": "^10.3.10",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.0.2",
    "rollup-plugin-natives": "^0.7.5",
    "rollup-plugin-node-externals": "^8.0.0"
  },
  "dependencies": {
    "@eniac/flexdesigner": "^1.0.1",
    "balanced-match": "^3.0.1",
    "brace-expansion": "^4.0.0",
    "decompress-response": "^9.0.0",
    "lru-cache": "^10.2.0",
    "mimic-response": "^4.0.0",
    "minimatch": "^9.0.3",
    "minipass": "^7.1.2",
    "once": "^1.4.0",
    "open": "^10.1.0",
    "parenthesis": "^3.1.8",
    "path-scurry": "^1.9.2",
    "simple-concat": "^1.0.1",
    "simple-get": "^4.0.1",
    "string-split-by": "^1.0.0",
    "wrappy": "^1.0.2",
    "yallist": "^5.0.0",
    "ytmdesktop-ts-companion": "^1.1.0"
  }
}
